{
    "manifest_version": 3,
    "name": "mfuko",
    "description": "Test",
    "options_page": "src/options.html",
    "background": {
        "service_worker": "dist/background.js",
        "type": "module"
    },
    "permissions": [
        "activeTab",
        "pageCapture",
        "scripting"
    ],
    "action": {},
